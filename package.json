{
  "name": "CoronaRadar",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "run-s test:*",
    "lint": "tslint .",
    "lint-prettier": "prettier --write '**/*.ts*'",
    "test:typecheck": "tsc",
    "test:lint": "tslint --project .",
    "test:prettier": "prettier --check --config .prettierrc \"src/**/*.ts*\"",
    "test:jest": "TEST=true jest",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/cli": "^3.0.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.17",
    "@types/react-native-indicators": "^0.16.0",
    "@types/react-redux": "^7.1.5",
    "axios": "^0.19.2",
    "class-transformer": "^0.2.3",
    "firebase": "^7.6.0",
    "lodash": "^4.17.15",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-indicators": "^0.17.0",
    "react-native-picker-select": "^6.6.0",
    "react-native-reanimated": "^1.7.1",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-navigation": "^4.0.10",
    "react-navigation-animated-switch": "^0.5.8",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.6.2",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^4.1.0",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-native": "^0.60.25",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^24.0.2",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "cacheDirectory": ".jest/cache",
    "collectCoverageFrom": [
      "src/*/*.ts",
      "src/*/*.tsx"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/e2e/",
      "__tests__/helpers/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "clover"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-fast-image|react-native-linear-gradient|react-native-blur|react-native-device-info|react-native-markdown-renderer|react-native-fabric|react-native-webview|react-native-action-sheet|react-native-image-crop-picker|react-native-thumbnail|react-native-splash-screen|react-native-vector-icons|react-native-safe-area-view|react-native-youtube|react-native-inappbrowser-reborn|react-native-reanimated|@react-native-community/async-storage|react-native-paper|react-native-firebase)/)"
    ],
    "setupFiles": [
      "<rootDir>/setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
